	<queryString>
		<![CDATA[select p.ID_PRODUTO, p.DESCRICAO, vp.QTD, 
       'R$ ' || to_char(vp.vlr_unit, '9999.99') as VLR_UNIT,
       'R$ ' || to_char(vp.vlr_total, '9999.99') as VLR_TOTAL,
	vp.vlr_total as VLR_DBL
 from venda v, venda_produtos vp , produto p
       where v.id_venda    = $P{p_id_venda}
       and   v.id_venda    = vp.id_venda
       and   vp.id_produto = p.id_produto
       order by p.descricao]]>
