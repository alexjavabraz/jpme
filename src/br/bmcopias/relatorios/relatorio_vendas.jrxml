	<queryString><![CDATA[select v.id_venda, v.vlr_venda, TO_CHAR(v.data_venda, 'dd/MM/YYYY') AS DATA_VENDA,
 cliente.nome as cliente, funcionario.nome as funcionario , funcionario_capta.nome as captador
  from venda v, cadastro funcionario, cadastro cliente, cadastro funcionario_capta
  where (v.data_venda) between to_date($P{p_data_inicial}, 'dd/MM/yyyy') and to_date($P{p_data_final}, 'dd/MM/yyyy')
  and    v.id_funcionario = funcionario.id_cadastro
  and    v.id_cliente     = cliente.id_cadastro
  and    v.id_funcionario_captacao = funcionario_capta.id_cadastro
   order by v.data_venda]]></queryString>

